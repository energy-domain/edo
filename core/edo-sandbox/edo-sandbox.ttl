#    Classes
#################################################################

###  https://w3id.org/energy-domain/edo-sandbox#BatteryEnergyStorage
sandbox:BatteryEnergyStorage rdf:type owl:Class ;
                             rdfs:subClassOf sandbox:EnergyStorageEquipment ;
                             dcterms:identifier "BatteryEnergyStorage" ;
                             skos:definition "Sistema que armazena energia elétrica em forma química para uso posterior, fornecendo equilíbrio da rede e energia de backup."@pt-br ,
                                             "System that stores electrical energy in chemical form for later use, providing grid balancing and backup power."@en ;
                             skos:prefLabel "Armazenamento de Energia em Bateria"@pt-br ,
                                            "Battery Energy Storage"@en ;
                             edo:hasDiscipline edo:ElectricalEngineering ,
                                               edo:EquipmentEngineering ;
                             edo:hasDomain edo:EnergyStorage .


###  https://w3id.org/energy-domain/edo-sandbox#BiofuelPlant

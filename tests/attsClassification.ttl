@prefix : <https://w3id.org/energy-domain/edo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

#################################################################
# Root Classification Superclass
#################################################################

:AttributeClassification rdf:type owl:Class ;
    rdfs:label "Attribute Classification"@en , "Classificação de Atributo"@pt-br ;
    skos:definition "Superclass for all classification categories of the Domain Attributes"@en ,
                    "Superclasse para todas as categorias de classificação dos Atributos do Domínio"@pt-br ;
    rdfs:comment "Superclass for all classification categories of the Domain Attributes."@en .

#################################################################
# 1) Scope of Applicability
#################################################################

:AttributeScope rdf:type owl:Class ;
    rdfs:subClassOf :AttributeClassification ;
    rdfs:label "Attribute Scope"@en , "Escopo do Atributo"@pt-br ;
    skos:definition "Defines whether the attribute applies to all instances of a type or to a specific instance"@en ,
                    "Define se o atributo se aplica a todas as instâncias de um tipo ou a uma instância específica"@pt-br ;
    rdfs:comment "Defines whether the attribute applies to all instances of a type or to a specific instance."@en .

:TypeLevelAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeScope ;
    rdfs:label "Type-Level Attribute"@en , "Atributo de Tipo"@pt-br ;
    skos:definition "Attribute common to all items of the same type"@en ,
                    "Atributo comum a todos os itens do mesmo tipo"@pt-br ;
    rdfs:comment "Attribute common to all items of the same type."@en .

:InstanceLevelAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeScope ;
    rdfs:label "Instance-Level Attribute"@en , "Atributo de Instância"@pt-br ;
    skos:definition "Attribute specific to a real-world individual instance of item"@en ,
                    "Atributo específico a uma instância individual no mundo real"@pt-br ;
    rdfs:comment "Attribute specific to a real-world individual instance of item."@en .

:BatchLevelAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeScope ;
    rdfs:label "Batch Attribute"@en , "Atributo de Lote"@pt-br ;
    skos:definition "Attribute common to a batch of equipment instances"@en ,
                    "Atributo comum a um lote de instâncias de equipamento"@pt-br ;
    rdfs:comment "Attribute common to a batch of equipment instances."@en .

#################################################################
# 2) Attribute Domain Category
#################################################################

:AttributeDomainCategory rdf:type owl:Class ;
    rdfs:subClassOf :AttributeClassification ;
    rdfs:label "Attribute Domain Category"@en , "Categoria de Domínio do Atributo"@pt-br ;
    skos:definition "Classifies the nature or domain of the attribute's content"@en ,
                    "Classifica a natureza ou domínio do conteúdo do atributo"@pt-br ;
    rdfs:comment "Classifies the nature or domain of the attribute's content."@en .

:FunctionalAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Functional Attribute"@en , "Atributo Funcional"@pt-br ;
    skos:definition "Represents operational characteristics and capabilities of the equipment"@en ,
                    "Representa características operacionais e capacidades do equipamento"@pt-br ;
    rdfs:comment "Describes how the equipment functions or performs its intended purpose."@en .

:DimensionalAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Dimensional Attribute"@en , "Atributo Dimensional"@pt-br ;
    skos:definition "Represents measurements such as length, diameter, volume"@en ,
                    "Representa medições como comprimento, diâmetro, volume"@pt-br ;
    rdfs:comment "Represents measurements such as length, diameter, volume."@en .

:IdentificationAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Identification Attribute"@en , "Atributo de Identificação"@pt-br ;
    skos:definition "Represents identifiers, codes, names"@en ,
                    "Representa identificadores, códigos, nomes"@pt-br ;
    rdfs:comment "Represents identifiers, codes, names."@en .

:HistoricalAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Historical Attribute"@en , "Atributo Histórico"@pt-br ;
    skos:definition "Represents records of past events, inspections, or modifications"@en ,
                    "Representa registros de eventos passados, inspeções ou modificações"@pt-br ;
    rdfs:comment "Represents records of past events, inspections, or modifications."@en .

:PhysicalPropertyAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Physical Property Attribute"@en , "Atributo de Propriedade Física"@pt-br ;
    skos:definition "Represents intrinsic physical properties such as mass, density, material"@en ,
                    "Representa propriedades físicas intrínsecas como massa, densidade, material"@pt-br ;
    rdfs:comment "Represents intrinsic physical properties such as mass, density, material."@en .

:PerformanceAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Performance Attribute"@en , "Atributo de Desempenho"@pt-br ;
    skos:definition "Represents performance or capacity values such as pressure rating or power"@en ,
                    "Representa valores de desempenho ou capacidade como pressão nominal ou potência"@pt-br ;
    rdfs:comment "Represents performance or capacity values such as pressure rating or power."@en .

:FinancialAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Financial Attribute"@en , "Atributo Financeiro"@pt-br ;
    skos:definition "Represents costs, prices, depreciation"@en ,
                    "Representa custos, preços, depreciação"@pt-br ;
    rdfs:comment "Represents costs, prices, depreciation."@en .

:StatusAttribute rdf:type owl:Class ;
    rdfs:subClassOf :AttributeDomainCategory ;
    rdfs:label "Status Attribute"@en , "Atributo de Status"@pt-br ;
    skos:definition "Represents operational state, availability, or maintenance condition"@en ,
                    "Representa estado operacional, disponibilidade ou condição de manutenção"@pt-br ;
    rdfs:comment "Represents operational state, availability, or maintenance condition."@en .

#################################################################
# 3) Value Origin
#################################################################

:ValueOrigin rdf:type owl:Class ;
    rdfs:subClassOf :AttributeClassification ;
    rdfs:label "Value Origin"@en , "Origem do Valor"@pt-br ;
    skos:definition "Indicates how the value is obtained"@en ,
                    "Indica como o valor é obtido"@pt-br ;
    rdfs:comment "Indicates how the value is obtained."@en .

:DeclaredValue rdf:type owl:Class ;
    rdfs:subClassOf :ValueOrigin ;
    rdfs:label "Declared"@en , "Declarado"@pt-br ;
    skos:definition "Manually specified value"@en ,
                    "Valor especificado manualmente"@pt-br ;
    rdfs:comment "Manually specified value."@en .

:CalculatedValue rdf:type owl:Class ;
    rdfs:subClassOf :ValueOrigin ;
    rdfs:label "Calculated"@en , "Calculado"@pt-br ;
    skos:definition "Derived from other values"@en ,
                    "Derivado de outros valores"@pt-br ;
    rdfs:comment "Derived from other values."@en .

:MeasuredValue rdf:type owl:Class ;
    rdfs:subClassOf :ValueOrigin ;
    rdfs:label "Measured"@en , "Medido"@pt-br ;
    skos:definition "Obtained by direct measurement"@en ,
                    "Obtido por medição direta"@pt-br ;
    rdfs:comment "Obtained by direct measurement."@en .

:ImportedValue rdf:type owl:Class ;
    rdfs:subClassOf :ValueOrigin ;
    rdfs:label "Imported"@en , "Importado"@pt-br ;
    skos:definition "Imported from another system"@en ,
                    "Importado de outro sistema"@pt-br ;
    rdfs:comment "Imported from another system."@en .

###############################################
###         Annotation Properties           ###
###############################################

### Scope Classification ###
:hasAttributeScope a owl:AnnotationProperty ;
    rdfs:label "has attribute scope"@en , "tem escopo de atributo"@pt-br ;
    skos:definition "Classifies whether the attribute applies to equipment type, instance or batch"@en ,
                    "Classifica se o atributo se aplica ao tipo de equipamento, instância ou lote"@pt-br .

### Category Classification ###
:hasAttributeCategory a owl:AnnotationProperty ;
    rdfs:label "has attribute category"@en , "tem categoria de atributo"@pt-br ;
    skos:definition "The technical category of the attribute (geometric, functional, etc.)"@en ,
                    "A categoria técnica do atributo (geométrica, funcional, etc.)"@pt-br .

### Lifecycle Phase ###
:hasLifecycleCreationPhase a owl:AnnotationProperty ;
    rdfs:label "has lifecycle creation phase"@en , "tem fase de criação no ciclo de vida"@pt-br ;
    skos:definition "The lifecycle phase when the attribute value is created"@en ,
                    "A fase do ciclo de vida quando o valor do atributo é criado"@pt-br .

:hasLifecycleUsagePhase a owl:AnnotationProperty ;
    rdfs:label "has lifecycle usage phase"@en , "tem fase de uso no ciclo de vida"@pt-br ;
    skos:definition "The lifecycle phase(s) when the attribute value is used"@en ,
                    "A(s) fase(s) do ciclo de vida quando o valor do atributo é utilizado"@pt-br .